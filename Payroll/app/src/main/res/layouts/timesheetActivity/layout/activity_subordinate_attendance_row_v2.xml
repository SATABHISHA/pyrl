    <LinearLayout
        android:id="@+id/ll_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginBottom="0dp"
        android:layout_marginTop="0dp"
        android:gravity="center_vertical"
        android:layout_gravity="center_vertical"
        android:background="#ffffff">
        <LinearLayout
            android:id="@+id/relative_layout"
            android:layout_width="match_parent"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:layout_marginBottom="0.6dp"
            android:background="#ffffff"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/tv_emp_name"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="nil-Aug-2020"
                android:layout_gravity="center_vertical"
                android:textColor="#565555"
                android:layout_marginRight="10dp"
                android:paddingRight="0dp"
                android:scrollHorizontally="false"
                android:layout_weight="1"
                android:textSize="15sp"/>
            <TextView
                android:id="@+id/tv_in_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:minWidth="60dp"
                android:text="10:00 AM"
                android:textColor="#565555"
                android:layout_weight="1"
                android:layout_gravity="center_vertical"
                android:textSize="13sp"/>
            <TextView
                android:id="@+id/tv_out_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:minWidth="60dp"
                android:text="5:30 PM"
                android:textColor="#565555"
                android:layout_weight="1"
                android:layout_gravity="center_vertical"
                android:textSize="13sp"/>
            <LinearLayout
                android:id="@+id/ll_label"
                android:layout_width="70dp"
                android:layout_height="30dp"
                android:layout_weight="0.2"
                android:layout_alignParentRight="true"
                android:layout_gravity="center_vertical"
                android:orientation="vertical"
                android:background="@drawable/loglist_corner_radius_present">
                <TextView
                    android:id="@+id/tv_present_absent"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:textColor="#ffffff"
                    android:text="Present"/>
            </LinearLayout>
            <ImageView
                android:id="@+id/img_sublog_arrow"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginRight="10dp"
                android:layout_gravity="center_vertical"
                android:src="@drawable/arrowrightsublog"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_border"
            android:layout_width="match_parent"
            android:layout_marginTop="0dp"
            android:layout_marginRight="40dp"
            android:layout_height="1dp"
            android:orientation="vertical"
            android:background="#D1D1D1"/>
    </LinearLayout>
</androidx.coordinatorlayout.widget.CoordinatorLayout>